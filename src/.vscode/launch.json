    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Api.Application/bin/Debug/netcoreapp3.1/application.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Api.Application",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "DB_CONNECTION_SQLSERVER": "Server=127.0.0.1,1433;Initial Catalog=dbSFEContatos;MultipleActiveResultSets=true; User Id=sa;Password=Adminmagti*1981",
                "DB_CONNECTION_MYSQL": "Server=localhost;Database=sfe_contatos;Uid=root;Pwd=Adminmagti*1981",
                "DATABASE": "MYSQL",
                "MIGRATION": "NAOAPLICAR",
                "Audience": "ExemploAudience",
                "Issuer": "ExemploIssuer",
                "DAYS": "1"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}